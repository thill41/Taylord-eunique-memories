<tr>
  <td><%= package.name %></td>
  <td><%= package.price %></td>
  <td><%= package.frequency %></td>
  <td><%= package.enabled ? 'Yes' : 'No' %></td>
  <td>
    <div class="admin-actions">
      <%= link_to :Edit, edit_package_path(package) %>
      <%= link_to :Delete, package, method: :delete, data: { tubo_confirm: t("destroy.confirm") } %>
    </div>
  </td>
</tr>